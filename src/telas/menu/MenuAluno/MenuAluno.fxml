<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Reflection?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="285.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="telas.menu.MenuAluno.ControllerMenuAluno">
   <children>
      <Label layoutX="224.0" layoutY="52.0" text="Menu do Aluno">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <Button fx:id="responder" layoutX="42.0" layoutY="154.0" mnemonicParsing="false" onAction="#responderSimulado" text="responder simulado" />
      <Button fx:id="atualizar" layoutX="444.0" layoutY="154.0" mnemonicParsing="false" onAction="#atualizar" text="atualizar aluno" />
      <Button fx:id="resultado" alignment="CENTER" layoutX="236.0" layoutY="154.0" mnemonicParsing="false" onAction="#resultSimulado" text="resultado do simulado" textAlignment="CENTER" />
      <Button fx:id="voltar" graphicTextGap="5.0" layoutX="63.0" layoutY="234.0" mnemonicParsing="false" onAction="#voltarAct" text="voltar">
         <effect>
            <Reflection topOpacity="0.17" />
         </effect>
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
